  salesforce.FormulaText npe01__Home_Address__c {
    apiName = "Contact.npe01__Home_Address__c"
    label = "Home Address"
    formula = '''
IF(
  ISPICKVAL(npe01__Primary_Address_Type__c,"Home"),
  IF(ISBLANK(MailingStreet), "", MailingStreet & ", ") & 
  IF(ISBLANK(MailingCity), "", MailingCity & ", ")& 
  IF(ISBLANK(MailingState), "", MailingState & " ")& 
  IF(ISBLANK(MailingPostalCode), "", MailingPostalCode) & 
  IF(ISBLANK(MailingCountry), "", ", " &MailingCountry)
,
IF(ISPICKVAL(npe01__Secondary_Address_Type__c,"Home"),
  IF(ISBLANK(OtherStreet), "", OtherStreet & ", ") & 
  IF(ISBLANK(OtherCity), "", OtherCity & ", ")& 
  IF(ISBLANK(OtherState), "", OtherState & " ")& 
  IF(ISBLANK(OtherPostalCode), "", OtherPostalCode) & 
  IF(ISBLANK(OtherCountry), "", ", " & OtherCountry)
  ,""
)
)
'''
    inlineHelpText = "Formula: Uses primary address type field to determine Home Address from Mailing or Other address."
    createable = false
    updateable = false
    unique = false
    caseSensitive = false
    length = 1300
    queryable = true
    deprecated = false
    description = "Formula: Uses primary address type field to determine Home Address from Mailing or Other address."
    externalId = false
    formulaTreatBlanksAs = "BlankAsZero"
    _hidden_value = true
  }
  salesforce.FormulaText npe01__Organization_Type__c {
    apiName = "Contact.npe01__Organization_Type__c"
    label = "Organization Type"
    formula = "NULLVALUE(IF( Account.npe01__SYSTEM_AccountType__c==\"\", TEXT(Account.Type),Account.npe01__SYSTEM_AccountType__c),\"Organization\" )"
